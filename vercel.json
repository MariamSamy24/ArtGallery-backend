{
    "version": 2,
    "builds": [
      { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/index.js" }
    ],
    "build": {
      "env": {
        "DATABASE_URL": "sql5.freesqldatabase.com"  // Set environment variables here if needed
      },
      "commands": {
        "build": "npm install && npm run build" // Replace this with your build command
      }
    }
  }
  